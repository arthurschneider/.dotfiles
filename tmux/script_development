tmux has-session -t script

if [ $? != 0 ];
then
    tmux new-session -s script -n editor -d

    tmux split-window -v -p 5 -t script
#    tmux select-layout -t script main-horizontal

    tmux send-keys -t script:0.0 'cd ~/Code' C-m
    tmux send-keys -t script:0.1 'cd ~/Code' C-m
    tmux send-keys -t script:0.0 'vim' C-m

    tmux new-window -n console -t script
    tmux send-keys -t script:1 'cd ~/Code' C-m

    tmux select-window -t script:0
fi

tmux attach -t script

